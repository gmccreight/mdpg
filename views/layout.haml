!!!
%html{"ng-app"=>"WnpApp"}
  %head
    %meta{name: "viewport",
      content: "width=320, initial-scale=1, user-scalable=1"}
    %script(src="/jquery.min.js")
    %link(rel="stylesheet" type="text/css" href="/style.css")

  %body
    - result = yield
    - if params[:debug] == "1"
      = $data_store.report
    = result

  %script(src="/angular/angular.min.js")
  %script(src="/angular/angular-resource.min.js")
  %script(src="/underscore-min.js")
  %script(src="/application.js")
  %script(src="/keyboard.js")
  %script(src="/other.js")
