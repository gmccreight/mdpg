#page_recent_ns_id

  = haml :header_partial, :locals => {:page_name => nil}

  %h1
    Edited

  - edited_pages.each do |page|
    %div
      %a{:href => "/p/#{page.name}"}
        = page.name

  %h1
    Viewed

  - viewed_pages.each do |page|
    %div
      %a{:href => "/p/#{page.name}"}
        = page.name
